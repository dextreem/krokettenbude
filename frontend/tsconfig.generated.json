{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ES2020", "DOM"],
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,

    "noUnusedLocals": false,
    "noUnusedParameters": false,

    "noEmit": true
  },
  "include": ["src/api-client"]
}
